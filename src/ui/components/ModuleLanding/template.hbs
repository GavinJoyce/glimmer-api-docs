<section class="content-container">
  <h5 class="project-breadcrumb">
    <EntityLink @name={{@project.name}} @projectId={{@project.slug}} />
  </h5>
  <div class="module-header">
    {{@module.kindString}} <h3>{{@module.name}}</h3>
  </div>
  {{#if @module.sources}}
    {{#unless @module.isFunction}}
      <SourceLink @sources={{@module.sources}} />
    {{/unless}}
  {{/if}}
  {{#if @module.extendedTypes}}
    <div class="hierarchy-meta">
    <strong>Extends</strong>

    {{#each @module.extendedTypes key="@index" as |extends|}}
      <TypeAnnotation @type={{extends}} />
    {{/each}}
    </div>
  {{/if}}

  {{#if @module.implementedTypes}}
    <div class="hierarchy-meta">
    <strong>Implements</strong>

    {{#each @module.implementedTypes key="@index" as |implements|}}
      <TypeAnnotation @type={{implements}} />
    {{/each}}
    </div>
  {{/if}}

  {{#if @module.comment}}
    <div class="readme">
      {{{@module.comment.text}}}
    </div>
  {{/if}}

  {{#if @module.constructors}}
    <div class="constructors">
      <h4>Constructor</h4>

      <MethodList @methods={{@module.constructors}} />
    </div>
  {{/if}}

  {{#if @module.methods}}
    <h4>Methods</h4>

    <MethodList @hasMethodCategories={{@module.hasMethodCategories}} @methods={{@module.methods}} />
  {{/if}}

  {{#if @module.signatures}}
    <div class="methods-list">
      <MethodItem @method={{@module}} />
    </div>
  {{/if}}

  {{#if @module.properties}}
    <VariableList @label="Properties" @variables={{@module.properties}} />
  {{/if}}
</section>
